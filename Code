d = []
while True:
    print('Digit (-1) to stop')
    n = int(input(':'))
    if n == -1:
        break
    i = 1
    a = 0
    while i <= n:
        if n % i == 0:
            d.append(i)
            a += 1

        i +=1
    if a == 2:
        print('Prime number')
    else:
        print('Is not a prime number')
        print(n,'Is divisible by:',list(set(d)))
